function _init()
	score_reset()
	title_init()
end

function _update()
	btnl=btn(0)
	btnr=btn(1)
	btnu=btn(2)
	btnd=btn(3)
	btnz=btn(4)
	btnx=btn(5)

	btnlp=btnp(0)
	btnrp=btnp(1)
	btnup=btnp(2)
	btndp=btnp(3)
	btnzp=btnp(4)
	btnxp=btnp(5)
	
	cart_update()
	t+=1
end

function _draw()
	cls()
	cart_draw()
	
	
	if debug then print(debug, 0,120, 1) print(debug, 1,121, 7) end
end




