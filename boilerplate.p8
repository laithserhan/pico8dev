--
-- #globals and system needs
--
t,screen_x,screen_y,screen_w,screen_h=0,0,0,127,127
ef=function() end
musicon=true

cart_update,cart_draw=ef,ef
cart_control=function(u,d)
	u=u or ef
	d=d or ef
	cart_update,cart_draw=u,d
	t=0
end



--
-- #player
--
function p_init()
	p_x,p_y,p_dx,p_dy,p_spd,p_acc=64,64,0,0,2,.3
	
end

function p_update()
	p_move()
	p_action()
end

function p_draw()
	print("\140",p_x,p_y, 10)
end

function p_move()
	p_dirx,p_diry=0,0

	if btnl	then p_dirx=-1 end
	if btnr	then p_dirx=1 end
	if btnu	then p_diry=-1 end
	if btnd	then p_diry=1 end
	
	p_dx*=p_acc
	p_dy*=p_acc
	p_dx+=p_spd*p_dirx
	p_dy+=p_spd*p_diry
	
	
	if not offscreen(p_x+p_dx,p_y+p_dy) then 
		p_x+=p_dx
		p_y+=p_dy
	end
end

